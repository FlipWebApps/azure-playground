{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "mhew-test-datafactory"
		},
		"DataHub_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datahub.io/"
		},
		"mhew-keyvault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://mhew-keyvault.vault.azure.net/"
		},
		"mhewstoragev2_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "data-factory-blob-connection"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"DataHub_properties_typeProperties_url": {
						"value": "[parameters('DataHub_properties_typeProperties_url')]"
					},
					"mhew-keyvault_properties_typeProperties_baseUrl": {
						"value": "[parameters('mhew-keyvault_properties_typeProperties_baseUrl')]"
					},
					"mhewstoragev2_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('mhewstoragev2_properties_typeProperties_connectionString_secretName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}